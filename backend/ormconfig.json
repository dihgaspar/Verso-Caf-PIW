{
    "type": "sqlite",
    "database": "database.sqlite",
    "synchronize": true,
    "logging": false,
    "entities": [
        "src/entity/**/*.ts"
    ],
    "migrations": [
        "src/migration/**/*.ts"
    ],
    "cli": {
    "migrationsDir": "src/migrations"
    },
    "subscribers": [
        "src/subscriber/**/*.ts"
    ]
}